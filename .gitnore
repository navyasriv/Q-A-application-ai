# =========================
# Node.js Project .gitignore
# =========================

# Dependencies
node_modules/
npm-debug.log*
yarn-debug.log*
yarn-error.log*
package-lock.json
pnpm-lock.yaml

# Build outputs
dist/
build/
.cache/
.next/
out/
coverage/

# Logs
logs/
*.log
*.pid
*.seed
*.pid.lock

# Environment files
.env
.env.local
.env.development.local
.env.test.local
.env.production.local

# Secrets / Keys
keys/
*.pem
*.key
*.crt
*.p12
*.json   # Prevents committing Google/AWS keys etc.

# OS-specific files
.DS_Store
Thumbs.db

# IDEs / Editors
.vscode/
.idea/
*.swp
*.swo
*.sublime-workspace
*.sublime-project
